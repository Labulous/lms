{
  "name": "labulous-lab-management",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "tsc --project tsconfig.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "api": "json-server --watch db.json --port 3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "migrate:products": "node scripts/migrate.js",
    "migrate:schema": "tsx scripts/migrations/01_create_case_tables.ts",
    "migrate:data": "tsx scripts/migrations/02_migrate_case_data.ts",
    "migrate:pontic": "tsx scripts/migrations/03_add_pontic_types.ts",
    "migrate:sql": "tsx scripts/migrations/execute_sql_migration.ts",
    "migrate:verify": "tsx scripts/migrations/verify_migration.ts",
    "migrate:verify-rls": "tsx scripts/migrations/verify_rls.ts",
    "migrate": "npm run migrate:schema && npm run migrate:data && npm run migrate:pontic",
    "start": "npm run build && npm run preview"
  },
  "dependencies": {
